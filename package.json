{
  "name": "node-sequelize",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "start": "node ./bin/www",
    "migrate": "sequelize db:migrate",
    "db:init": "sequelize init",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@sentry/node": "^6.5.0",
    "country-state-city": "^3.0.1",
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
