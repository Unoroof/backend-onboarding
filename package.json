{
  "name": "node-sequelize",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "start": "node server.js",
    "serve": "ENVFILE=.env DEBUG=true nodemon server.js",
    "migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:init": "sequelize init",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@locospec/engine": "^0.0.34",
    "@locospec/operator-knexjs": "^0.0.14",
    "@sentry/node": "^6.5.0",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "country-state-city": "^3.0.1",
    "dayjs": "^1.10.7",
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "knex": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "morgan": "^1.10.0",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "nodemon": "^2.0.7"
  }
}
